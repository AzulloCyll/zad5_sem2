{
	"name": "mini-starterkit",
	"version": "1.0.0",
	"main": "index.js",
	"license": "MIT",
	"scripts": {
		"build:dev": "cross-env parcel src/html/*.html --open",
		"build:prod": "cross-env NODE_ENV=production parcel build src/html/*.html --out-dir public --public-url ./ --no-cache --no-source-maps --no-minify"
	},
	"devDependencies": {
		"autoprefixer": "^9.1.3",
		"core-js": "^3.9.0",
		"cross-env": "^5.2.0",
		"node-sass": "^4.9.3",
		"normalize.css": "^8.0.1",
		"npm-run-all": "^4.1.5",
		"parcel-bundler": "^1.12.3",
		"parcel-plugin-clean-easy": "^1.0.2",
		"postcss-modules": "^1.3.2",
		"regenerator-runtime": "^0.13.7",
		"sass": "^1.32.8"
	},
	"parcelCleanPaths": [
		"dist",
		"public"
	],
	"dependencies": {}
}
